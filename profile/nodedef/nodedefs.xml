<nodeDefs>
    <!-- NODE DEF from 5.0 document http://wiki.universal-devices.com/index.php?title=V50_MAIN -->
    <!-- controller -->
    <nodeDef id="controller" nls="ctl">
        <editors />
        <sts>
			<st id="ST" editor="bool" />
            <st id="GV1" editor="bool" />
		</sts>
        <cmds>
            <sends />
            <accepts>
                <cmd id="DISCOVER" />
            </accepts>
        </cmds>
    </nodeDef>
	<!-- mynodetype -->
    <nodeDef id="somfyshade" nls="shade">
        <editors />
        <sts>
            <st id="ST" editor="shd_pos" />
            <st id="GV1" editor="seconds" />
        </sts>
        <cmds>
            <sends />
            <accepts>
                <cmd id="DON">
					<p id="" editor="shd_pos" init="ST" />
				</cmd>
                <cmd id="DOF" />
				<cmd id="BRT" />
				<cmd id="DIM" />
                <cmd id="STOP" />
                <cmd id="QUERY" />
                <cmd id="SET_TRAVEL_TIME">
                    <p id="" editor="seconds" init="GV3" />
                </cmd>
            </accepts>
        </cmds>
    </nodeDef>
</nodeDefs>